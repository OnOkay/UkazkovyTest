<Window x:Class="UkazkovyTest.View.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:UkazkovyTest.View"
        mc:Ignorable="d"
        Title="Login" Height="233" Width="299">
    <Grid>
        <PasswordBox Name="PasswordBox" HorizontalAlignment="Left" Margin="60,126,0,0" VerticalAlignment="Top" Width="178" Grid.ColumnSpan="2" Height="18"/>
        <TextBox Name="UsernameBox" HorizontalAlignment="Center" Margin="0,83,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="178"/>
        <Label Content="Please enter your username and password&#xD;&#xA;" HorizontalAlignment="Left" Margin="36,10,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
        <Label Content="Username&#xD;&#xA;" HorizontalAlignment="Left" Margin="118,52,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.295,0.393" Grid.ColumnSpan="2"/>
        <Label Content="Password&#xA;" HorizontalAlignment="Left" Margin="117,101,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.295,0.393" Grid.ColumnSpan="2"/>
        <Button Name="LoginButton" Content="Log in" HorizontalAlignment="Left" Margin="200,173,0,0" VerticalAlignment="Top"/>
        <Button Name="QuitButton" Content="Quit" HorizontalAlignment="Left" Margin="60,173,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
